    "flow-bin": "^0.89.0",
    "jest": "^23.6.0",
    "regenerator-runtime": "^0.13.1",
    "rollup": "^1.1.0",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-uglify": "^6.0.0"
  },
  "dependencies": {
    "oky": "0.0.1",
    "ramda": "^0.26.1",
    "ramda-validations": "^1.0.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "extensions": [
            ".jsx",
            ".ts"
          ],
          "root": [
            "./src"
          ]
        }
      ]
    ]
  },
  "jest": {
    "moduleDirectories": [
      "./node_modules",
      "./src"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(js|ts)$": "babel-jest"
    }
  }
}
